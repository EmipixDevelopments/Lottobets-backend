  <script>
		    $(document).ready(function() {
				$(".addLink").click(function(){
					let url = $(this).attr("data-url");
					let urltype = $(this).attr("data-urltype");
					let lottoid = $(this).attr("data-lottoid");
					let lottoName = $(this).attr("data-lottoname");
					let country = $(this).attr("data-country");
					let state = $(this).attr("data-state");
					let liveURL = $(this).attr("data-liveurl");
					let drawLink = $(this).attr("data-drawlink");
					let drawTime = $(this).attr("data-drawtime");
					let result = $(this).attr("data-result");
					//$(".modal-title").text(lottoName);
					$(".modal-title").html("<b>"+lottoName+"</b>");
					$(".lottoId").val(lottoid);
					$(".lottoName").val(lottoName);
					$(".country").val(country);
					$(".state").val(state);
					$(".result").val(result);
					$(".drawTime").val(drawTime);
					$(".drawLink").val(drawLink);
					
					
				});
				$(".close").click(function(){
					let url = "";
				});			
			});	
		</script>
